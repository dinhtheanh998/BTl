@model BTL_LTQL.Models.Category

@{
    ViewBag.Title = "Delete";
    Layout = "~/Areas/Admins/Views/Shared/Admin_LoaiSp.cshtml";
}
<div class="Reg-log-wrap">
    <h3>Bạn chắc chắn muốn xóa loại sản phẩm này?</h3>
    <div class="Reg-log-wrap">
        <hr />
        <dl class="dl-horizontal">
            <dt>
                Tên loại : 
            </dt>

            <dd>
                @Html.DisplayFor(model => model.CategoryName)
            </dd>

        </dl>
        <div class="divider h54"></div>
        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()
            <div class="form-actions no-color">
                <input type="submit" value="Delete" class="btn btn-primary text-white handle-btn" /> |
                <a class="btn btn-primary mr-xl-auto" href="/Admins/Categories/Index">Quay Lại</a>
            </div>
        }
    </div>
</div>
